<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.company.taximgmt.web.mainwindow.ExtAppMainWindow"
        extends="/com/haulmont/cuba/web/app/mainwindow/mainwindow.xml"
        messagesPack="com.company.taximgmt.web.mainwindow"
        xmlns:ext="http://schemas.haulmont.com/cuba/window-ext.xsd"
        xmlns:main="http://schemas.haulmont.com/cuba/mainwindow.xsd">
    <layout>
        <hbox id="titleBar">
            <label align="MIDDLE_CENTER"
                   ext:index="2"
                   htmlEnabled="true"
                   stylename="colored"
                   value="DASHBOARD"/>
            <main:userIndicator id="userIndicator"
                                align="MIDDLE_RIGHT"/>
        </hbox>
        <split id="foldersSplit">
            <main:workArea id="workArea">
                <main:initialLayout>
                    <hbox>
                        <label htmlEnabled="true"
                               icon="ss.png"/>
                        <label htmlEnabled="true"
                               icon="car.png"/>
                        <label htmlEnabled="true"
                               icon="rating.png"/>
                    </hbox>
                    <hbox>
                        <label htmlEnabled="true"
                               icon="map.png"
                               value="msg://map"/>
                    </hbox>
                </main:initialLayout>
            </main:workArea>
        </split>
    </layout>
</window>
