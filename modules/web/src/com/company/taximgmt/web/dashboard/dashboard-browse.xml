<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="com.company.taximgmt.web.dashboard.DashboardBrowse"
        messagesPack="com.company.taximgmt.web.dashboard">
    <dsContext>
        <collectionDatasource id="dashboardsDs"
                              class="com.company.taximgmt.entity.Dashboard"
                              view="_local">
            <query>
                <![CDATA[select e from taximgmt$Dashboard e]]>
            </query>
        </collectionDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout spacing="true">
        <groupBox caption="Embedded"
                  height="250px"
                  spacing="true"
                  width="250px">
            <embedded id="embedded"
                      align="MIDDLE_CENTER"
                      height="100%"
                      width="100%"/>
        </groupBox>
    </layout>
</window>
