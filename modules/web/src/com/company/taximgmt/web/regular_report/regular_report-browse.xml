        <collectionDatasource id="regular_reportsDs"
                              class="com.company.taximgmt.entity.Regular_report"
                              view="_local">
            <query>
                <![CDATA[select e from taximgmt$Regular_report e]]>
            </query>
        </collectionDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="regular_reportsTable"
            spacing="true">
        <table id="regular_reportsTable"
               width="100%">
            <actions>
            </actions>
            <columns>
            </columns>
            <rows datasource="regular_reportsDs"/>
