<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="com.company.taximgmt.gui.emp_car_details.Emp_car_detailsBrowse"
        focusComponent="emp_car_detailsesTable"
        lookupComponent="emp_car_detailsesTable"
        messagesPack="com.company.taximgmt.gui.emp_car_details">
    <dsContext>
        <collectionDatasource id="emp_car_detailsesDs"
                              class="com.company.taximgmt.entity.Emp_car_details"
                              view="_local">
            <query>
                <![CDATA[select e from taximgmt$Emp_car_details e]]>
            </query>
        </collectionDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="emp_car_detailsesTable"
            spacing="true">
        <filter id="filter"
                applyTo="emp_car_detailsesTable"
                datasource="emp_car_detailsesDs">
            <properties include=".*"/>
        </filter>
        <table id="emp_car_detailsesTable"
               width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
                <action id="excel"/>
            </actions>
            <columns>
                <column id="emp_id"/>
                <column id="emp_car_no"/>
                <column id="date"/>
                <column id="source"/>
                <column id="source_time"/>
                <column id="destination"/>
                <column id="des_time"/>
                <column id="rating"/>
            </columns>
            <rows datasource="emp_car_detailsesDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="emp_car_detailsesTable.create"/>
                <button id="editBtn"
                        action="emp_car_detailsesTable.edit"/>
                <button id="removeBtn"
                        action="emp_car_detailsesTable.remove"/>
                <button id="excelBtn"
                        action="emp_car_detailsesTable.excel"/>
            </buttonsPanel>
        </table>
    </layout>
</window>
